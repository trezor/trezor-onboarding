{
  "name": "trezor-onboarding",
  "version": "1.0.0",
  "repository": "https://github.com/trezor/trezor-onboarding",
  "author": "SatoshiLabs s.r.o.",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "dev": "webpack-dev-server --env.BUILD=development",
    "lint": "run-s lint:*",
    "lint:js": "npx eslint --ext .js,.jsx ./src/",
    "lint:ts": "npx eslint --ext .ts,.tsx --no-eslintrc --config .ts.eslintrc ./src/",
    "lint:css": "npx stylelint './src/**/*.js'",
    "build:development": "rimraf dist && npx webpack --progress --bail --env.BUILD=development",
    "build:beta": "rimraf dist && npx webpack --progress --bail --env.BUILD=beta",
    "build:production": "rimraf dist && npx webpack --progress --bail --env.BUILD=production",
    "test-all": "yarn test-unit && yarn test-health",
    "test-unit": "jest --testPathPattern=src/utils",
    "test-health": "jest --testPathPattern=test/health",
    "test-e2e": "npx cypress run --env updateSnapshots=true",
    "test-e2e:update": "npx cypress run --env updateSnapshots=true",
    "translations:extract": "rimraf translations/extractedMessages && cross-env NODE_ENV=translations babel src && yarn ttm merge-msgs && yarn ttm build-csv",
    "translations:upload": "yarn translations:extract && yarn ttm upload",
    "translations:download": "yarn ttm build-translations && sleep 1 && yarn ttm export-translations",
    "check-types": "tsc --jsx react"
  },
  "dependencies": {
    "@sentry/browser": "^4.6.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "connected-react-router": "^6.3.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-intl": "v3.0.0-beta-1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.2",
    "react-svg": "^7.2.6",
    "react-timeout": "^1.2.0",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.1",
    "redux-localstorage-simple": "^2.1.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.3",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.4",
    "styled-reset": "^1.6.4",
    "trezor-connect": "^7.0.3",
    "trezor-flags": "^1.0.2",
    "trezor-ui-components": "1.0.0-beta.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-intl": "^2.3.17",
    "@types/react-redux": "7.0.1",
    "@types/react-svg": "^5.0.0",
    "@types/styled-components": "4.1.8",
    "@typescript-eslint/eslint-plugin": "^1.0.0",
    "@typescript-eslint/parser": "^1.7.0",
    "add": "^2.0.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-intl": "^3.0.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "cypress-image-snapshot": "^3.0.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "fork-ts-checker-webpack-plugin": "^1.3.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "node-fetch": "^2.3.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.5.1",
    "svg-inline-loader": "^0.8.0",
    "trezor-translations-manager": "^1.0.4",
    "typescript": "^3.3.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "yarn": "^1.16.0"
  }
}
